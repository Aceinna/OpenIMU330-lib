{
	"name": "OpenIMU330-base-library",
 	"version": "1.0.5",
 	"description": "OpenIMU330 Base Library",
    "keywords": "sensors, IMU, INS, AI, automotive, navigation",
 	"repository":
    {
       "type": "git",
       "url": "https://github.com/Aceinna/openIMU330-lib.git",
       "branch": "master"
    },
	"build": {
 		"flags": [
			"-L Sensors",
			"-l libSensors.a",
			"-I Sensors",
			"-I Platform/CAN/include",
			"-I Platform/Filter/include",
			"-I Platform/Board/include",
			"-I Platform/Core/include",
			"-I Platform",
			"-I STM32L431/Inc",
			"-I STM32L431/CMSIS",
			"-I STM32L431",
 			"-I Baremetal",
			"-I Baremetal/include"
		],
		"libArchive": false,
        "platforms": "aceinna_imu"
 	}
 }
